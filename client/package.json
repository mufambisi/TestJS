{
  "name": "micro-app-client",
  "version": "1.2.0",
  "description": "Project Micro App Client Application",
  "main": "dist/index.js",
  "author": "Origin Digital",
  "license": "UNLICENSED",
  "scripts": {
    "build": "NODE_ENV=production yarn build:assets --config ./webpack/webpack.config.prod.ts && yarn build:copy-conf",
    "build:assets": "webpack -r ../../../../devTools/scripts/register-ts.js",
    "build:copy-conf": "node -r ./devTools/scripts/register-ts.js ./config/generate-ci-config.ts",
    "build:local": "yarn clean && NODE_ENV=local && yarn build:assets --config ./webpack/webpack.config.local.ts",
    "clean": "rm -fr coverage dist *.log",
    "coverage:report": "open ./coverage/lcov-report/index.html",
    "lint": "yarn lint:client && yarn lint:test:func",
    "lint:client": "origin-scripts lint",
    "lint:test:func": "cd ./functional-test && yarn lint",
    "fixture:generate": "node -r ./devTools/scripts/register-ts.js ./devTools/scripts/generate-fixtures.ts",
    "format": "origin-scripts format",
    "format:check": "origin-scripts format --check --no-write",
    "graphql:schema:download": "node -r ./devTools/scripts/register-ts.js devTools/scripts/download-schema.ts",
    "graphql:clean:rx-gateway:types": "rm -rf ./src/graphql-types/rx-gateway/*.ts",
    "graphql:generate:rx-gateway:types": "yarn graphql:clean:rx-gateway:types && apollo codegen:generate ./src/graphql-types/rx-gateway --localSchemaFile=devTools/schema/rx-gateway/schema.json --target=typescript --includes=src/dashboard/queries.ts --addTypename --globalTypesFile=src/graphql-types/rx-gateway/globalTypes.ts --outputFlat --mergeInFieldsFromFragmentSpreads",
    "graphql:generate:types": "yarn graphql:generate:rx-gateway:types",
    "serve": "yarn ws --directory dist --spa index.html --port 8080 --rewrite \"/microsite/template/api/* -> http://${STUB_HOST:=localhost}:3001/microsite/template/api/\\$1\"",
    "serve:stub": "digital-stub",
    "start:webpack": "webpack-dev-server -r ../../../../devTools/scripts/register-ts.js --config ./webpack/webpack.config.dev.ts",
    "start": "ENV=local node -r ./devTools/scripts/register-ts.js ./devTools/scripts/dev-server.ts",
    "stub": "node  -r ./devTools/scripts/register-ts.js ./devTools/scripts/load-mocks.ts",
    "test": "origin-scripts test --all --coverage",
    "test:func": "node -r ./devTools/scripts/register-ts.js ./functional-test/scripts/test-func.ts",
    "test:func:applitools": "yarn test:func --applitools",
    "test:func:docker": "yarn test:func --docker",
    "test:func:live": "yarn test:func -l",
    "test:func:debug": "testcafe $NODE_DEBUG_OPTION chrome",
    "test:watch": "yarn test --watch --no-coverage",
    "type:check": "tsc --noEmit",
    "validate": "origin-scripts validate && yarn format:check && yarn type:check"
  },
  "dependencies": {
    "@origin-digital/auth-utils": "1.3.0",
    "@origin-digital/battery-widget": "5.3.4",
    "@origin-digital/broadband-widget": "2.3.2",
    "@origin-digital/ev-widget": "2.3.0",
    "@origin-digital/event-dispatcher": "5.12.1",
    "@origin-digital/features": "11.1.5",
    "@origin-digital/fuel-discount-widget": "0.1.1",
    "@origin-digital/kraken-account": "2.7.0",
    "@origin-digital/kraxi": "2.1.8",
    "@origin-digital/lpg-widget": "6.10.2",
    "@origin-digital/ods-core": "1.16.3",
    "@origin-digital/ods-icons": "1.8.5",
    "@origin-digital/ods-lab": "1.8.0",
    "@origin-digital/personalisation": "8.11.0",
    "@origin-digital/reporting-client": "4.5.0",
    "@origin-digital/spike-widget": "2.3.2",
    "@origin-digital/toggle": "2.0.0",
    "@origin-digital/widget-provider": "0.3.0",
    "date-fns": "^2.16.1",
    "lodash.sortby": "^4.7.0",
    "sumo-logger": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "7.4.4",
    "@babel/runtime-corejs2": "^7.1.2",
    "@origin-digital/babel-preset": "1.3.1",
    "@origin-digital/daxi": "2.8.22",
    "@origin-digital/digital-stub": "^7.2.2",
    "@origin-digital/kraken-fixture": "^0.0.19",
    "@origin-digital/node-http-client": "^0.2.1",
    "@origin-digital/node-logger": "^4.0.6",
    "@origin-digital/node-util": "^4.0.4",
    "@origin-digital/ods-testing-library": "1.1.1",
    "@origin-digital/origin-scripts": "^1.13.0",
    "@origin-digital/webpack-plugin-tal-develop-fs": "^8.1.0",
    "@testing-library/jest-dom": "4.1.2",
    "@testing-library/react": "^10.4.8",
    "@types/enzyme": "^3.1.10",
    "@types/js-yaml": "^3.12.4",
    "@types/lodash.flatten": "^4.4.6",
    "@types/lodash.sortby": "^4.7.6",
    "@types/minimist": "^1.2.0",
    "@types/node-fetch": "^2.5.6",
    "@types/numeral": "^0.0.28",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/string-similarity": "^4.0.0",
    "@types/sumo-logger": "^1.6.2",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "apollo": "^2.32.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "copy-webpack-plugin": "^4.5.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.4",
    "glob": "^7.2.0",
    "graphql": "^15.3.0",
    "https-proxy-agent": "^5.0.0",
    "js-yaml": "^3.14.0",
    "local-web-server": "^2.5.6",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-test-renderer": "16.9.0",
    "styled-components": "3.4.5",
    "svg-react-loader": "^0.4.5",
    "testcafe": "^1.8.1",
    "testcafe-browser-provider-browserstack": "^1.11.0",
    "testcafe-reporter-slack": "^0.0.6",
    "tslib": "^2.1.0",
    "typescript": "^3.8.2",
